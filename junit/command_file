create Folder=1
create Folder=Child ParentID=lFolder1ID
create VCSRepository=1
vcscheckout Folder1 VCSRepository1
vcsadd FolderChild
get Composition1=Composition path=Folder1 listid=true
get Target1=Target path=Folder1 listid=true
move Composition FolderChild
move Target FolderChild
vcscommit Folder1
move FolderChild FolderParent
checkmodifiedflag Composition1
checkmodifiedflag Target1
# vcs Commit
done